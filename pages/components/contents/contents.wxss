  0% {
      transform: rotate(0deg);
  }

  to {
      transform: rotate(1turn);
  }
}

@keyframes a {
  0% {
      transform: rotate(0deg);
  }

  to {
      transform: rotate(1turn);
  }
}

.weui-loadmore__tips {
  color: var(--weui-FG-0);
  display: inline-block;
  vertical-align: middle;
}

.weui-loadmore {
  font-size: 14px;
  line-height: 1.6em;
  margin: 1.5em auto;
  text-align: center;
  width: 65%;
}

.weui-loadmore_line {
  border-top: 1px solid var(--weui-FG-3);
  margin-top: 2.4em;
}

.weui-loadmore_line .weui-loadmore__tips {
  background-color: var(--weui-BG-2);
  color: var(--weui-FG-1);
  padding: 0 .55em;
  position: relative;
  top: -.9em;
}

.item-class {
  background-color: #fff;
  display: flex;
  flex-flow: row nowrap;
  margin-bottom: 2rpx;
  padding-top: 30rpx;
}

.item-border-class {
  border-bottom: 1rpx solid #e5e5e5;
}

.userinfo-avatar {
  background-color: #e5e5e5;
  border-radius: 10%;
  display: block;
  height: 86rpx;
  margin-left: 24rpx;
  overflow: hidden;
  width: 86rpx;
}

.userinfo-nickname {
  color: #576b95;
  display: block;
  font-size: 34rpx;
  font-weight: 600;
}

.item-right-class {
  display: flex;
  flex: 1;
  flex-flow: column;
  margin-left: 20rpx;
  margin-right: 20rpx;
  width: 0;
}

.item-text-class {
  align-self: baseline;
  background-color: #fff;
  color: #191919;
  font-size: 34rpx;
  line-height: 44rpx;
  overflow: hidden;
  padding: 8rpx 0;
}

.all_no_class {
  -webkit-line-clamp: 5;
}

.all_class {
  -webkit-line-clamp: 0;
}

.item-image-class {
  background-color: #eee;
  margin-top: 26rpx;
  width: 55%;
}

.item-btn {
  justify-content: left;
  width: 100%;
}

.item-btn,.item-btn button {
  display: flex;
}

.item-btn button {
  align-items: center;
  background-color: #fff;
  color: grey;
  flex: 1;
  font-size: 30rpx;
  font-weight: 400;
  margin-bottom: 1rpx;
  padding: 20rpx 0rpx;
  width: 0;
}

.item-btn button:active {
  color: #a5a5a5;
}

.item-btn button::after {
  border: none;
}

.item-btn-class {
  justify-content: center;
}

.num_class {
  font-size: 24rpx;
  margin-left: 4rpx;
}

.item-btn image {
  height: 50rpx;
  margin-right: 10rpx;
  width: 50rpx;
}

.item-btn-load {
  margin-right: 20rpx;
}

.show_class {
  justify-content: left;
  width: 68rpx;
}

.show_class,.show_class button {
  display: flex;
}

.show_class button {
  background-color: #fff;
  color: #576b95;
  flex: 1;
  font-size: 34rpx;
  font-weight: 400;
  line-height: inherit;
  margin-top: 10rpx;
  padding: 0rpx;
}

.show_class button::after {
  border: none;
}

.show_class button:active {
  background-color: #e5e5e5;
}

.ad {
  border-bottom: 1px solid #eee;
  padding-bottom: 10rpx;
  padding-top: 30rpx;
}

.adContainer,.takeaway_class {
  margin-left: 3%;
  margin-right: 3%;
  width: 94%;
}

.takeaway_class image {
  height: 500rpx;
  width: 100%;
}